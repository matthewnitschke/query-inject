{
  "name": "Query Inject",
  "version": "1.0.2",
  "icons": { 
    "16": "16.png",
    "48": "48.png",
   "128": "128.png" 
  },
  "manifest_version": 2,
  "options_page": "dist/options/index.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "Query Inject",
    "default_popup": "dist/options/index.html"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["<all_urls>"],
      "js": ["dist/inject/inject.js"]
    }
  ]
}
